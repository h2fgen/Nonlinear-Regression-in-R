Time=c(0.25, 0.5, 1, 1.5, 2, 3, 4, 6, 8, 12)
Conc=c(57.4, 54, 44.8, 52.7, 43.6, 40.1, 27.9, 20.6, 15, 10)
summary(nls(Conc~C0*exp(-ke*Time), start=list(C0=50, ke=0.2)))
summary(nls(Conc~(1000/Vd)*2^(-Time/tHalf), start=list(Vd=200, tHalf=4)))
plot(Conc ~ Time)
lines(lowess(Conc ~ Time))
m2 <- lm(Conc ~ poly(Time, 2, raw=TRUE))
m3 <- lm(Conc ~ Time + I(Time^2))
lines(Time, predict(m2))
lines(Time, predict(m2), col=2)
